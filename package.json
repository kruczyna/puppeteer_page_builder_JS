{
  "name": "pptr-mocha",
  "version": "1.0.0",
  "description": "test framework with mocha steps and builder class pattern",
  "main": "index.js",
  "scripts": {
    "test": "node ./node_modules/mocha/bin/mocha --timeout=30000 --require mocha-steps ./build/tests/**.js",
    "clean": "del -rf build",
    "build": "babel --preset es2015 -d build/ src/"
  },
  "keywords": [
    "mocha",
    "puppeteer",
    "framework"
  ],
  "author": "Victoria Kruczek",
  "license": "ISC",
  "dependencies": {
    "chai": "^4.2.0",
    "cucumber": "^6.0.5",
    "cucumber-html-reporter": "^5.2.0",
    "mocha": "^7.1.2",
    "mocha-steps": "^1.3.0",
    "puppeteer": "^3.0.4"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-es2015": "^6.24.1"
  }
}
